.forwading-inner-sec{padding: 30px 15px;}
.forwading-inner-sec .switch {
  position: relative;
  display: inline-block;
  width: 60px;
  height: 30px;
}

.forwading-inner-sec .switch input {display:none;}

.forwading-inner-sec .slider {
  position: absolute;
  cursor: pointer;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #6E6E6E;
  -webkit-transition: .4s;
  transition: .4s;
}

.forwading-inner-sec .slider:before {
  position: absolute;
  content: "";
  height: 24px;
  width: 24px;
  left: 4px;
  bottom: 4px;
  background-color: white;
  -webkit-transition: .4s;
  transition: .4s;
}

.forwading-inner-sec input:checked + .slider {
  background-color: #0cc9d3;
}

.forwading-inner-sec input:focus + .slider {
  box-shadow: 0 0 1px #0cc9d3;
}

.forwading-inner-sec input:checked + .slider:before {
  -webkit-transform: translateX(26px);
  -ms-transform: translateX(26px);
  transform: translateX(26px);
}

/* Rounded sliders */
.forwading-inner-sec .slider.round {
  border-radius: 34px;
}

.forwading-inner-sec .slider.round:before {
  border-radius: 50%;
}
